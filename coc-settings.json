{
    "coc.preferences.hoverTarget": "float",
    "coc.preferences.snippets.enable": true,
    "coc.preferences.snippetStatusText": "â“ˆ ",
    "coc.preferences.colorSupport": true,
    "coc.preferences.enableFloatHighlight": true,
    "coc.preferences.extensionUpdateCheck": "daily",
    "coc.source.vimtex.enable": true,
    "suggest.noselect": false,
    "suggest.autoTrigger": "always",
    "suggest.triggerAfterInsertEnter": true,
    "suggest.minTriggerInputLength": 1,
    "suggest.enablePreview": true,
    "suggest.snippetIndicator": "",
    "suggest.timeout": 5000,
    "suggest.echodocSupport": true,
    "suggest.floatEnable": true,
    "suggest.preferCompleteThanJumpPlaceholder": false,
    "suggest.acceptSuggestionOnCommitCharacter": true,
    "suggest.completionItemKindLabels": {
		    "class": "\uf0e8",
		    "color": "\ue22b",
		    "constant": "\uf8fe",
		    "default": "\uf29c",
		    "enum": "\uf435",
		    "enumMember": "\uf02b",
		    "event": "\ufacd",
		    "field": "\uf93d",
		    "file": "\uf723",
		    "folder": "\uf115",
		    "function": "\u0192",
		    "interface": "\uf417",
		    "keyword": "\uf1de",
		    "method": "\uf6a6",
		    "module": "\uf40d",
		    "operator": "\uf915",
		    "property": "\ue624",
		    "reference": "\ufa46",
		    "snippet": "\ue60b",
		    "struct": "\ufb44",
		    "text": "\ue612",
		    "typeParameter": "\uf728",
		    "unit": "\uf475",
		    "value": "\uf89f",
		    "variable": "\ue71b"
	    },
    "clangd.checkUpdates": true,
    "clangd.semanticHighlighting": true,
    "python.jediEnabled": true,
    "python.autoComplete.addBrackets": true,
    "yaml.format.enable": true,
    "snippets.ultisnips.directories": [
	    "~/.config/nvim/Ultisnips/",
	    "~/.config/nvim/plugged/vim-snippets/UltiSnips/"
    ],
	"xml.java.home": "/usr/lib/jvm/java-14-openjdk",
    "yank.highlight.duration": 100,
    "yank.enableCompletion": false,
    "explorer.keyMappingMode": "none",
    "explorer.openAction.strategy": "split",
    "explorer.file.column.indent.indentLine": true,
    "explorer.file.showHiddenFiles": false,
    "explorer.icon.enableNerdfont": true,
    "explorer.file.column.git.showIgnored": true,
    "explorer.keyMappings": {
	"<space>": "toggleSelection",
	"m": "actionMenu",

	"k": "nodeNext",
	"i": "nodePrev",
	"j": "gotoParent",
	"l": "cd",
	"e": ["expanded?", "collapse", "expand"],
	"o": ["expandable?", "expand", "open"],
	"gj": "expandRecursive",
	"gl": "collapseRecursive",
	"gi": "expandablePrev",
	"gk": "expandableNext",

	"<cr>": ["expanded?", "collapse", "expand"],

	"s": "open:split",
	"S": "open:vsplit",
	"t": "open:tab",
	"I": "gotoParent",

	"yp": "copyFilepath",
	"yn": "copyFilename",
	"yf": "copyFile",
	"c": "cutFile",
	"p": "pasteFile",
	"d": "delete",
	"D": "deleteForever",

	"af": "addFile",
	"ad": "addDirectory",
	"r": "rename",

	"h": "toggleHidden",
	"R": "refresh",

	"?": "help",
	"q": "quit",
	"<esc>": "esc",
	"x": "systemExecute",
	"gd": "listDrive",

	"f": "search",
	"F": "searchRecursive",

	"gf": "gotoSource:file",
	"gb": "gotoSource:buffer",

	"[[": "sourcePrev",
	"]]": "sourceNext",

	"[d": "diagnosticPrev",
	"]d": "diagnosticNext",

	"[c": "gitPrev",
	"]c": "gitNext",
	"<<": "gitStage",
	">>": "gitUnstage"
    }
}
